<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zms.listcard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivitySmallImageCard" />
        <activity android:name=".ActivityBigImageCard" />
        <activity android:name=".ActivityBasicImageButtonCard" />
        <activity android:name=".ActivityWelcomeCard" />
        <activity android:name=".ActivityBasicListCard" />
        <activity android:name=".ActivityBigImageButtonCard" />
    </application>

</manifest>
